{% extends "layout.html.twig" %}

{% block keywords %}404, Not found{% endblock %}

{% block title %}{{ website.title }} - 404 Page Not Found{% endblock %}

{% block page_title %}
    <!-- Header of the webpage. -->
    <header class="scims-page-header col-md-12 col-sm-12 col-xs-12">
        <h1 class="scims-page-title">
            {{ website.title }} : {{ message.message }}
        </h1>
    </header>
{% endblock %}

{% block content %}
    <section class="col-md-8 col-sm-8 col-xs-8">
        <p>{{ message.message }}</p>
    </section>
{% endblock %}
